{
    "name": "1tomany/pdf-to-image-bundle",
    "description": "Symfony bundle for the 1tomany/pdf-to-image library",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "pdf",
        "poppler",
        "pdftoppm",
        "xpdf",
        "pdf to image",
        "pdf2img",
        "pdf2image"
    ],
    "require": {
        "php": ">=8.4",
        "1tomany/pdf-to-image": "^0.4.0",
        "symfony/dependency-injection": "^7.2",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87.2",
        "phpstan/phpstan": "^2.1.23"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\PdfToImageBundle\\": "src/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
