{
    "name": "1tomany/pdf-ai-bundle",
    "description": "Symfony bundle for the 1tomany/pdf-ai library",
    "license": "MIT",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "ai",
        "llm",
        "pdf",
        "pdf2img",
        "pdf2image",
        "pdf2text",
        "pdf2txt",
        "pdfinfo",
        "pdftoppm",
        "pdf to image",
        "pdf to text",
        "poppler",
        "xpdf"
    ],
    "require": {
        "php": ">=8.2",
        "1tomany/pdf-ai": "^0.5.1",
        "symfony/dependency-injection": "^7.2",
        "symfony/http-kernel": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.90",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\PDFAIBundle\\": "src/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
